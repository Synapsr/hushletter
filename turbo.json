{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", ".vinxi/**", ".expo/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^generate"]
    },
    "dev:web": {
      "cache": false,
      "persistent": true
    },
    "dev:convex": {
      "cache": false,
      "persistent": true
    },
    "generate": {
      "cache": false
    },
    "deploy": {
      "dependsOn": ["^deploy"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    }
  }
}
